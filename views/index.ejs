<!doctype html>
<html lang="en">
  
  <head>
    <meta charset="utf-8" lang="en">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Notes & Projects</title>
    <link rel="shortcut icon" type="image/png" href="/img/fav_notes.png">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Kalam&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.2/themes/base/jquery-ui.css">
    <link href="/css/styles.css" rel="stylesheet">
  </head>

  <body>
    <div hidden id="wtr_simple" style="height: 0;"><%=wtr_simple_PH%></div>
    <div hidden id="celsius" style="height: 0;"><%=celsius_PH%></div>
    <div hidden id="mili_diff" style="height: 0;"><%=mili_diff_PH%></div>
    <div hidden id="tmz_suffix" style="height: 0;"><%=tmz_suffix_PH%></div>
    <div hidden id="hour_offset" style="height: 0;"><%=hour_offset_PH%></div>
    
    <div id="shine-div">      
      <div class="container">

        <div class="container" id="hero-greeting">
          <div class="row">
            <div class="col" id="title-box">
              <h1>Good <span id="period_of_day">day</span>, <span id="span_username"><%=first_name_PH%></span></h1>
              <div id="logout-box">
                <svg xmlns="http://www.w3.org/2000/svg" width="25" height="25" fill="currentColor" class="bi bi-door-closed" viewBox="0 0 16 16" style="margin-top: 5px;">
                  <path d="M3 2a1 1 0 0 1 1-1h8a1 1 0 0 1 1 1v13h1.5a.5.5 0 0 1 0 1h-13a.5.5 0 0 1 0-1H3zm1 13h8V2H4z"/>
                  <path d="M9 9a1 1 0 1 0 2 0 1 1 0 0 0-2 0"/>
                </svg>
              </div>
              <form hidden action="/home" method="post" name="logout-form" id="logout-form">
                <input hidden type="number" name="logout" id="logout" value=1>
              </form>
            </div>
          </div>
        </div>
  
        <div class="container main-container">
          <div class="row text-center row-days dayA" id="day1">

            <div class="col col-days daytime">
              <h5><%=dayA_PH%></h5>
              <h4 hidden class="hidden_date day1 daytime"><%=dayA_hidden_date_PH%></h4>
              <div class="notes-box">
                <%let notesDayA_PH = JSON.parse(notesDayA_PH_string);%>
                <%for (i = 0; i < notesDayA_PH.length; i++){%>
                  <header>
                    <div class="wk stamp" style="position: fixed"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bookmark-fill" viewBox="0 0 16 16">
                      <path d="M2 2v13.5a.5.5 0 0 0 .74.439L8 13.069l5.26 2.87A.5.5 0 0 0 14 15.5V2a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2z"/>
                    </svg></div>
                    <div class="mh stamp" style="position: fixed"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bookmark-fill" viewBox="0 0 16 16">
                      <path d="M2 2v13.5a.5.5 0 0 0 .74.439L8 13.069l5.26 2.87A.5.5 0 0 0 14 15.5V2a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2z"/>
                    </svg></div>
                    <div class="header__bg"></div>
                    <textarea class="expandable-textarea added_note day1 daytime" id="added_note_day1_daytime<%=i%>" style="width:100%" oninput='this.style.height = "";this.style.height = this.scrollHeight + "px"' onblur='this.style.height = "";this.style.height = this.scrollHeight + "px"' onfocus='this.style.height = "";this.style.height = this.scrollHeight + "px"' autocomplete="off"><%=notesDayA_PH[i][0]%></textarea>
                  <div hidden class="note_timestamp"><%=notesDayA_PH[i][1]%></div>
                </header>
                <%}%>
                <header>
                  <div class="header__bg"></div>
                  <textarea class="expandable-textarea new_note" id="new_note_adder1" placeholder="new note" style="width:100%" oninput='this.style.height = "";this.style.height = this.scrollHeight + "px"' onblur='this.style.height = "";this.style.height = this.scrollHeight + "px"' onfocus='this.style.height = "";this.style.height = this.scrollHeight + "px"' autocomplete="off" maxlength="80"></textarea>
                </header>
              </div>
              <form action="/home" method="post" class="form-day" id="form-day1" hidden>
                <div class="expandable-textarea" role="textbox" id="new_note_dayA" data-text="new note" autocomplete="off"></div>
              </form>
            </div> <!-- class="col col-days daytime">-->

            <div class="col col-days ngttime">
              <h5><%=dayB_PH%></h5>
              <h4 hidden class="hidden_date day1 ngttime"><%=dayB_hidden_date_PH%></h4>
              <div class="notes-box">
                <%let notesDayB_PH = JSON.parse(notesDayB_PH_string)%>
                <%for (j = 0; j < notesDayB_PH.length; j++){%>
                <header>
                  <div class="wk stamp" style="position: fixed"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bookmark-fill" viewBox="0 0 16 16">
                    <path d="M2 2v13.5a.5.5 0 0 0 .74.439L8 13.069l5.26 2.87A.5.5 0 0 0 14 15.5V2a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2z"/>
                  </svg></div>
                  <div class="mh stamp" style="position: fixed"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bookmark-fill" viewBox="0 0 16 16">
                    <path d="M2 2v13.5a.5.5 0 0 0 .74.439L8 13.069l5.26 2.87A.5.5 0 0 0 14 15.5V2a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2z"/>
                  </svg></div>
                  <div class="header__bg"></div>
                  <textarea class="expandable-textarea added_note day1 ngttime" id="added_note_day1_ngttime<%=j%>" style="width:100%" oninput='this.style.height = "";this.style.height = this.scrollHeight + "px"' onblur='this.style.height = "";this.style.height = this.scrollHeight + "px"' onfocus='this.style.height = "";this.style.height = this.scrollHeight + "px"' autocomplete="off"><%=notesDayB_PH[j][0]%></textarea>
                  <div hidden class="note_timestamp"><%=notesDayB_PH[j][1]%></div>
                </header>
                <%}%>
                <header>
                  <div class="header__bg"></div>
                  <textarea class="expandable-textarea new_note" id="new_note_adder1" placeholder="new note" style="width:100%" oninput='this.style.height = "";this.style.height = this.scrollHeight + "px"' onblur='this.style.height = "";this.style.height = this.scrollHeight + "px"' onfocus='this.style.height = "";this.style.height = this.scrollHeight + "px"' autocomplete="off" maxlength="80"></textarea>
                </header>
              </div>
              <form action="/home" method="post" class="form-day" id="form-day2" hidden>
                <div class="expandable-textarea" role="textbox" id="new_note_dayA" data-text="new note" autocomplete="off"></div>
              </form>
            </div> <!-- class="col col-days ngttime">-->

            <div class="weather_day" id="weather_day1">

              <%var current_hour=local_hour_PH;%>
              <%const weather=JSON.parse(weather_PH);%>
              <%var dayA_wtr, dayB_wtr, dayC_wtr;%>
              <%if(3 < current_hour && current_hour < 17 && dayA_hidden_date_PH == today_YYYYMMDD_PH){%>

                <%if(weather[dayA_hidden_date_PH]){%>
                  <%dayA_wtr=weather[dayA_hidden_date_PH];%>
                  <div class="hourly_weather">
                    <%for(let f = 1; f < 7; f++){%>
                      <div class="hour_weather">
                        <%let wtr_f=dayA_wtr["hr_tmp_code"][current_hour+f];%>
                        <p class="hour_hour_weather"><%=wtr_f[0]%></p>
                        <%let codes_f = wtr_f[2];%>
                        <%if (codes_f < 3){%>
                          <%if (dayA_wtr.sunset > current_hour+f && current_hour+f > dayA_wtr.sunrise){%>
                            <%if (codes_f == 0 || codes_f == 1){%>
                              <svg class="weather_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-sun-fill" viewBox="0 0 16 16">
                                <path d="M8 12a4 4 0 1 0 0-8 4 4 0 0 0 0 8zM8 0a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-1 0v-2A.5.5 0 0 1 8 0zm0 13a.5.5 0 0 1 .5.5v2a.5.5 0 0 1-1 0v-2A.5.5 0 0 1 8 13zm8-5a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1 0-1h2a.5.5 0 0 1 .5.5zM3 8a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1 0-1h2A.5.5 0 0 1 3 8zm10.657-5.657a.5.5 0 0 1 0 .707l-1.414 1.415a.5.5 0 1 1-.707-.708l1.414-1.414a.5.5 0 0 1 .707 0zm-9.193 9.193a.5.5 0 0 1 0 .707L3.05 13.657a.5.5 0 0 1-.707-.707l1.414-1.414a.5.5 0 0 1 .707 0zm9.193 2.121a.5.5 0 0 1-.707 0l-1.414-1.414a.5.5 0 0 1 .707-.707l1.414 1.414a.5.5 0 0 1 0 .707zM4.464 4.465a.5.5 0 0 1-.707 0L2.343 3.05a.5.5 0 1 1 .707-.707l1.414 1.414a.5.5 0 0 1 0 .708z"/>
                              </svg>
                            <%} else if (codes_f == 2){%>
                              <svg class="weather_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-cloud-sun-fill" viewBox="0 0 16 16">
                                <path d="M11.473 11a4.5 4.5 0 0 0-8.72-.99A3 3 0 0 0 3 16h8.5a2.5 2.5 0 0 0 0-5h-.027z"/>
                                <path d="M10.5 1.5a.5.5 0 0 0-1 0v1a.5.5 0 0 0 1 0v-1zm3.743 1.964a.5.5 0 1 0-.707-.707l-.708.707a.5.5 0 0 0 .708.708l.707-.708zm-7.779-.707a.5.5 0 0 0-.707.707l.707.708a.5.5 0 1 0 .708-.708l-.708-.707zm1.734 3.374a2 2 0 1 1 3.296 2.198c.199.281.372.582.516.898a3 3 0 1 0-4.84-3.225c.352.011.696.055 1.028.129zm4.484 4.074c.6.215 1.125.59 1.522 1.072a.5.5 0 0 0 .039-.742l-.707-.707a.5.5 0 0 0-.854.377zM14.5 6.5a.5.5 0 0 0 0 1h1a.5.5 0 0 0 0-1h-1z"/>
                              </svg>
                            <%}%>
                          <%}else{%>
                            <%if (codes_f == 0){%>
                              <svg class="weather_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-moon-stars-fill" viewBox="0 0 16 16">
                                <path d="M6 .278a.768.768 0 0 1 .08.858 7.208 7.208 0 0 0-.878 3.46c0 4.021 3.278 7.277 7.318 7.277.527 0 1.04-.055 1.533-.16a.787.787 0 0 1 .81.316.733.733 0 0 1-.031.893A8.349 8.349 0 0 1 8.344 16C3.734 16 0 12.286 0 7.71 0 4.266 2.114 1.312 5.124.06A.752.752 0 0 1 6 .278z"/>
                                <path d="M10.794 3.148a.217.217 0 0 1 .412 0l.387 1.162c.173.518.579.924 1.097 1.097l1.162.387a.217.217 0 0 1 0 .412l-1.162.387a1.734 1.734 0 0 0-1.097 1.097l-.387 1.162a.217.217 0 0 1-.412 0l-.387-1.162A1.734 1.734 0 0 0 9.31 6.593l-1.162-.387a.217.217 0 0 1 0-.412l1.162-.387a1.734 1.734 0 0 0 1.097-1.097l.387-1.162zM13.863.099a.145.145 0 0 1 .274 0l.258.774c.115.346.386.617.732.732l.774.258a.145.145 0 0 1 0 .274l-.774.258a1.156 1.156 0 0 0-.732.732l-.258.774a.145.145 0 0 1-.274 0l-.258-.774a1.156 1.156 0 0 0-.732-.732l-.774-.258a.145.145 0 0 1 0-.274l.774-.258c.346-.115.617-.386.732-.732L13.863.1z"/>
                              </svg>
                            <%} else if (codes_f == 1){%>
                              <svg class="weather_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-moon-fill" viewBox="0 0 16 16">
                                <path d="M6 .278a.768.768 0 0 1 .08.858 7.208 7.208 0 0 0-.878 3.46c0 4.021 3.278 7.277 7.318 7.277.527 0 1.04-.055 1.533-.16a.787.787 0 0 1 .81.316.733.733 0 0 1-.031.893A8.349 8.349 0 0 1 8.344 16C3.734 16 0 12.286 0 7.71 0 4.266 2.114 1.312 5.124.06A.752.752 0 0 1 6 .278z"/>
                              </svg>                     
                            <%} else if (codes_f == 2){%>
                              <svg class="weather_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-cloud-moon-fill" viewBox="0 0 16 16">
                                <path d="M11.473 11a4.5 4.5 0 0 0-8.72-.99A3 3 0 0 0 3 16h8.5a2.5 2.5 0 0 0 0-5h-.027z"/>
                                <path d="M11.286 1.778a.5.5 0 0 0-.565-.755 4.595 4.595 0 0 0-3.18 5.003 5.46 5.46 0 0 1 1.055.209A3.603 3.603 0 0 1 9.83 2.617a4.593 4.593 0 0 0 4.31 5.744 3.576 3.576 0 0 1-2.241.634c.162.317.295.652.394 1a4.59 4.59 0 0 0 3.624-2.04.5.5 0 0 0-.565-.755 3.593 3.593 0 0 1-4.065-5.422z"/>
                              </svg>
                            <%}%>
                          <%}%>
                        <%} else if (codes_f == 3){%>
                          <svg class="weather_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-clouds-fill" viewBox="0 0 16 16">
                            <path d="M11.473 9a4.5 4.5 0 0 0-8.72-.99A3 3 0 0 0 3 14h8.5a2.5 2.5 0 1 0-.027-5z"/>
                            <path d="M14.544 9.772a3.506 3.506 0 0 0-2.225-1.676 5.502 5.502 0 0 0-6.337-4.002 4.002 4.002 0 0 1 7.392.91 2.5 2.5 0 0 1 1.17 4.769z"/>
                          </svg>
                        <%} else if (codes_f == 45){%>
                          <svg class="weather_icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-cloud-fog-fill" viewBox="0 0 16 16">
                            <path d="M3 13.5a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 0 1h-9a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 0 1h-9a.5.5 0 0 1-.5-.5zm10.405-9.473a5.001 5.001 0 0 0-9.499-1.004A3.5 3.5 0 1 0 3.5 12H13a3 3 0 0 0 .405-5.973z"/>
