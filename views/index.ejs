            <%let days_31 = JSON.parse(days_31_PH);%>
            <div class="col col-days" id="next_7_days">
              <div class="specific_next_event" id="next_events_title">
                <h5>In 2~7 days:</h5>
              </div>              
                <%for (let d = 0; d < days_7.length; d++){%>
                  <%let buf_day = days_7[d];%>
                  <div class="specific_next_event">
                    <p class="next_event_event"><%=buf_day[0]%></p>
                    <div class="next_event_date-box">
                      <%for (let i = 0; i < buf_day[1].length; i++){%>
                      <p class="next_event_date"><%=buf_day[1][i].slice(0,11)%></p>
                      <%}%>
                    </div>
                  </div>
                <%}%>
            </div>
            <div class="col col-days" id="next_30_days">
              <div class="specific_next_event" id="next_events_title">
                <h5>In 8~31 days:</h5>
              </div>
              <%for (let e = 0; e < days_31.length; e++){%>
                <%let buf_day = days_31[e];%>
                <div class="specific_next_event">
                  <p class="next_event_event"><%=buf_day[0]%></p>
                  <div class="next_event_date-box">
                    <%for (let i = 0; i < buf_day[1].length; i++){%>
                    <p class="next_event_date"><%=buf_day[1][i].slice(0,11)%></p>
                    <%}%>
                  </div>
                </div>
              <%}%>
            </div>
          </div>          
          
          <div class="row text-center row-days dayC" id="projects_section">
            <div id="projects-container">
              <%let projects = JSON.parse(projects_PH_str)%>
              <%if (projects.length < 1){%>
                <div id="empty_projects-box">
                  <p>PROJECTS AREA</p>
                </div>
                <%} else{%>
                  <div class="projects-box">
                    <%for (m = 0; m < projects.length; m++){%>                      
                      <div class="project" id="project<%=m%>">
                        <p class="project_title"> <%=projects[m].title%> </p>
                        <div class="tasks">
                          <%if(projects[m]["tasks_done"].length){%>
                            <%let done=projects[m]["tasks_done"];%>
                            <%for(let e = 0; e < done.length; e++){%>
                              <div class="task_with_lines">
                                <hr class="done_line line">
                                <div class="specific_task done">
                                  <div class="obs-box">
                                    <div class="obs-header">
                                      <h4 class="obs-title">Observations</h4>
                                      <button class="obs-save-button">SAVE</button>
                                    </div>
                                    <textarea name="obs_done<%=e%>" class="obs" id="obs_done<%=e%>" cols="30" rows="10"><%=done[e]["obs"]%></textarea>
                                  </div>
                                  <p class="task-text"><%=done[e]["task"]%></p>
                                    <%if (done[e]["deadline"].length == 10){%>
                                      <p class="task-deadline"><%=new Date(done[e]["deadline"]).toUTCString().slice(5,16)%></p>
                                      <p hidden class="task-deadline-YYYY-MM-DD"><%=done[e]["deadline"]%></p>
                                    <%}%>
                                </div>
                                <hr class="done_line line">
                              </div>
                          <%}%>
                        <%}%>

                        <%if (projects[m]["tasks_todo"].length){%>
                          <%let todo=projects[m]["tasks_todo"];%>
                          <%for (n = 0; n < todo.length; n++){%>
                            <div class="task_with_lines">
                              <hr class="todo_line line">
                              <div class="specific_task todo">
                                <div class="obs-box">
                                  <div class="obs-header">
                                    <h4 class="obs-title">Observations</h4>
                                    <button class="obs-save-button">SAVE</button>
                                  </div>
                                  <textarea name="obs_todo<%=n%>" class="obs" id="obs_todo<%=n%>" cols="30" rows="10"><%=todo[n]["obs"]%></textarea>
                                </div>
                                <p class="task-text"><%=todo[n]["task"]%></p>
                                <%if (todo[n]["deadline"]){%>
                                <p class="task-deadline"><%=new Date(todo[n]["deadline"]).toUTCString().slice(5,16)%></p>
                                <p hidden class="task-deadline-YYYY-MM-DD"><%=todo[n]["deadline"]%></p>
                                <%}%>
                              </div>
                              <hr class="todo_line line">
                            </div>
                          <%}%>  
                        <%}%>
                        </div>
                        <%if (projects[m]["final_deadline"]){%>
                          <%let buffer_ddl = new Date(projects[m]["final_deadline"]).toUTCString();%>
                        <div class="final_deadline-box"><%=buffer_ddl.slice(5,16)%></div>
                        <%}%>
                      </div>
                      <%}%>                  
                  </div>
                  <%}%>
                  <button type="button" id="add_new_project">Add new project</button>
            </div>
                        
          </div> <!-- class="row text-center row-days projects_section" id="projects_section" -->
          
