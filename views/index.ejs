                <input class="new_project_deadline" name="new_task_deadline0" id="new_task_deadline0" form="form-new_project" placeholder="DEADLINE" type="text" onfocusin="(this.type='date')" onfocusout="(this.type='text')">  
                <button type="button" class="add_another_task">+1</button>
