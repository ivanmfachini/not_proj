            <form action="/login" method="post" name="login-form" class="login-form" id="login-form" class="login_buttons">
              <input type="text" name="username" id="username" placeholder="username" autocomplete="off" maxlength="30" autofocus required>
              <input type="text" name="realname" id="realname" placeholder="your first name" autocomplete="off" maxlength="20" required>
              <input type="password" name="password" id="password" placeholder="password" maxlength="50" required>
              <input type="password" name="password-confirm" id="password-confirm" placeholder="confirm password" autocomplete="off" required>
              <div id="privacy_policy_warning" style="display: flex; width: 90%; align-self: center;">
                <input type="radio" id="policy_accept" name="policy_accept" value=1 style="width: 16px;" required/>
                  <label for="policy_accept" style="padding-left: 12px; font-size: 0.9rem; align-self: center;">I have read and accept the <a href="/privacy_policy">Privacy Policy</a><br>Li e aceito a <a href="/politica_de_privacidade">Pol√≠tica de Privacidade</a></label>
              </div>
              <div class="row-buttons">
                <button type="button" name="login" id="login">Login</button>
                <button type="button" form="register-form" name="register" id="register">Register</button>
              </div>
              <div class="row-buttons">
                <button type="submit" name="lost_access" id="lost_access" form="lost_access-form">Lost Access</button>
                <button type="button" class="btn-hover color-3" id="demo" form="demo-form">Demonstration</button>
              </div>
            </form>
